{
  "name": "jcbowen/mysql-helper-yii2",
  "description": "基于yii2的mysql维护助手，可以将表结构以数组方式储存下来，也可以轻松实现表的差异对比及生成对应的修复sql语句",
  "keywords": [
    "yii2",
    "jcbowen",
    "MysqlHelperYii2",
    "Mysql Helper"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jcbowen\\MysqlHelperYii2\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Bowen",
      "email": "bowen@jiuchet.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "^2.0",
    "ext-pdo": "*"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
